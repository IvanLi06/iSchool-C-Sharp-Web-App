<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Ivan's iSchool Web App</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!-- jquery ui -->
    <link rel="stylesheet" href="~/lib/jquery-ui/jquery-ui.min.css"/>
    <!-- jquery datatable ui plugin -->
    <link rel="stylesheet" href="~/lib/jquery-data-table/datatables.min.css"/>
    <!-- jquery zebra dialog ui plugin -->
    <link rel="stylesheet" href="~/lib/jquery-zebra-dialog/dist/css/classic/zebra_dialog.css"/>
    <link rel="stylesheet" href="~/lib/jquery-zebra-dialog/dist/css/classic/zebra_dialog.min.css" />
    <link rel="stylesheet" href="~/lib/jquery-button/jquery-rxHtmlButton-0.3-min.css"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ISchoolAppWebAppIvan.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Ivan's iSchool Web App</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="About">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Degrees">Degrees</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Minors">Minors</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Employment">Employment</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="People">People</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Ivan's iSchool Web App
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <!-- jquery ui -->
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <!-- jquery datatable ui plugin -->
    <script src="~/lib/jquery-data-table/datatables.min.js"></script>
    <!-- jquery zebra dialog ui plugin -->
    <script src="~/lib/jquery-zebra-dialog/dist/zebra_dialog.min.js"></script>
    <script src="~/lib/jquery-zebra-dialog/dist/zebra_dialog.src.js"></script>
    <script src="~/lib/jquery-button/jquery-rxHtmlButton-0.3-min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
